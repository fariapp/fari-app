            {
              "sections": [
                {
                  "id": "4675bfa5-3b23-42dd-9a41-a67e84e63736",
                  "label": "EMV ABILITIES",
                  "blocks": [
                    {
                      "id": "74c3091f-baac-47ea-b6ce-c945d5d561b5",
                      "label": "EMV XP",
                      "type": "PointCounter",
                      "meta": {
                        "max": "8",
                        "isMainPointCounter": false,
                        "helperText": "At the end of a shift, mark EMV XP for each of the following, up to twice each:<br>- You brought a client safely home.<br>- You answered a dangerous dispatch.<br>- You made a run with no bloodshed.<br>Once you've accumulated 8XP, clear the EMV XP marks and pick a new EMV ABILITY below.<br>"
                      },
                      "value": "0"
                    },
                    {
                      "id": "ec367d38-3fd9-4564-83c7-e11dd879a889",
                      "label": "EMV ABILITIES",
                      "type": "Text",
                      "meta": {
                        "helperText": "Pick one to start. If you can't decide, pick either \"Tough Old Bird\" or \"Lucky Old Jack.\" "
                      }
                    },
                    {
                      "id": "6a0c0bef-8d13-4ed3-afd1-1586a7c679ad",
                      "label": "Field Repairs",
                      "type": "Text",
                      "value": "Crewmembers have the edge when patching/hacking the EMV.",
                      "meta": { "checked": false }
                    },
                    {
                      "id": "b26fcae1-13c4-4751-a6ed-fb7770eb92a7",
                      "label": "Hearts &amp; Minds",
                      "type": "Text",
                      "value": "Once per shift, any player may introduce a friendly NPC with the edge.",
                      "meta": {}
                    },
                    {
                      "id": "3478e2a5-2b58-487d-b6c8-2cf7f3851fce",
                      "label": "Lucky Old Jack",
                      "type": "Text",
                      "value": "If a joker is drawn, any player may discard a jack from their cache to shuffle it back into the deck.",
                      "meta": { "checked": false }
                    },
                    {
                      "id": "1112051c-3455-441b-a619-3d36542793a5",
                      "label": "Saints Of The Coast",
                      "type": "Text",
                      "value": "At end of shift, crewmembers gain 1XP if they put themselves at risk for a client.",
                      "meta": { "checked": false }
                    },
                    {
                      "id": "901b10c9-32a0-4892-b120-9c673c00b2ee",
                      "label": "School Bus",
                      "type": "Text",
                      "value": "Your crew is learning from each other. Each character may increase an action rating by one, as long as another character has a higher rating in it.",
                      "meta": {}
                    },
                    {
                      "id": "9114f73b-e77b-40e3-8538-a63532802eb9",
                      "label": "Shining Sight",
                      "type": "Text",
                      "value": "Your EMV is a welcome presence in the Cali skies. Gain the dge on the first interaction with a new person or group.",
                      "meta": {}
                    },
                    {
                      "id": "59128501-1d6a-47a7-85b4-cf704e1eac9e",
                      "label": "Speed Holes",
                      "type": "Text",
                      "value": "Fill in one section of the Maintenance clock permanently (next page).",
                      "meta": { "checked": false }
                    },
                    {
                      "id": "61f51cf3-d827-41b1-93c7-49780de61f59",
                      "label": "Tough Old Bird",
                      "type": "Text",
                      "value": "Any harm the EMV takes is reduced by one level.",
                      "meta": { "checked": false }
                    }
                  ]
                }
              ]
            }
