                    {
                      "id": "355b1347-6ec2-422d-8f33-b0e1b6a0d37d",
                      "label": "Close",
                      "type": "Text",
                      "value": "A few steps away at most. In the same hex.",
                      "meta": {}
                    },
                    {
                      "id": "74bd00e2-2ee1-44eb-8736-2363aa6f1dec",
                      "label": "Near",
                      "type": "Text",
                      "value": "A short distance away. One hex away.",
                      "meta": {}
                    },
                    {
                      "id": "58be5961-db78-4797-8f1f-580a5b458ba1",
                      "label": "Far",
                      "type": "Text",
                      "value": "Beyond Near but within range. Two hexes away.",
                      "meta": {}
                    },
                    {
                      "id": "8924c79c-a4f8-4a6a-bc25-e92d2f33b1e8",
